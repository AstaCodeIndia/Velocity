    <script src="https://cdn.jsdelivr.net/gh/automat/controlkit.js@master/bin/controlKit.min.js"></script>


    <script type="text/javascript" src="https://unpkg.com/sheryjs/dist/Shery.js"></script> <!-- Recommended -->


    <script src="/javascripts/main.js"></script>
    <script>
        gsap.to('#about-text h1', {
            x: "-100%",
            duration: 8,
            repeat: "2"
        })
        Shery.imageEffect("#moving-img", {
            style: 3,
            // debug: true,
            gooey: true,
            config: { "uFrequencyX": { "value": 12, "range": [0, 100] }, "uFrequencyY": { "value": 11.45, "range": [0, 100] }, "uFrequencyZ": { "value": 4.58, "range": [0, 100] }, "geoVertex": { "range": [1, 64], "value": 5.33 }, "zindex": { "value": -9996999, "range": [-9999999, 9999999] }, "aspect": { "value": 1.8941878842910884 }, "ignoreShapeAspect": { "value": true }, "shapePosition": { "value": { "x": 0, "y": 0 } }, "shapeScale": { "value": { "x": 0.5, "y": 0.5 } }, "shapeEdgeSoftness": { "value": 0, "range": [0, 0.5] }, "shapeRadius": { "value": 0, "range": [0, 2] }, "currentScroll": { "value": 0 }, "scrollLerp": { "value": 0.07 }, "gooey": { "value": true }, "infiniteGooey": { "value": true }, "growSize": { "value": 4, "range": [1, 15] }, "durationOut": { "value": 1, "range": [0.1, 5] }, "durationIn": { "value": 1.5, "range": [0.1, 5] }, "displaceAmount": { "value": 0.5 }, "masker": { "value": true }, "maskVal": { "value": 1.06, "range": [1, 5] }, "scrollType": { "value": 0 }, "noEffectGooey": { "value": true }, "onMouse": { "value": 0 }, "noise_speed": { "value": 0.2, "range": [0, 10] }, "metaball": { "value": 0.2, "range": [0, 2], "_gsap": { "id": 24 } }, "discard_threshold": { "value": 0.63, "range": [0, 1] }, "antialias_threshold": { "value": 0, "range": [0, 0.1] }, "noise_height": { "value": 0.5, "range": [0, 2] }, "noise_scale": { "value": 19.85, "range": [0, 100] } }
        });
        SplitType.create('.animated-text h2')
        gsap.from('.animated-text h2 .word .char', {
            y: 150,
            duration: .5,
            stagger: .1,
            scrollTrigger: {
                trigger: '.animated-text',
                start: 'top 80%',
                end: "top 80%",
            }
        })
        SplitType.create('.animated-para p')
        gsap.from('.animated-para p .word', {
            y: 200,
            duration: .5,
            stagger: .1,
            scrollTrigger: {
                trigger: '.animated-para',
                start: 'top 80%',
                end: "top 80%",
            }
        })
    </script>
